import java.io.*;

class InteractiveIO {
	public InteractiveIO() {
	}

	public void write(String s) {
		System.out.println(s);
		System.out.flush();
	}

	public String promptAndRead(String s) throws Exception {
		System.out.println(s);
		System.out.flush();
		BufferedReader br;
		br = new BufferedReader(new InputStreamReader(System.in));
		String line;
		line = br.readLine();
		return line;
	}
}