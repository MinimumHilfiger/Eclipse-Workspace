package v3_Package;
import java.io.*;
import java.net.*;
import java.util.Scanner;

public class Filter {

	public static void main(String[] arg) throws Exception {
		Scanner sc = new Scanner(System.in);
		System.out.println("Bitte geben Sie ein Muster ein.");
		String muster = sc.nextLine();
		String aktuelleZeile = "";
		
		URL u = new URL("https://www.whitehouse.gov/");
		FilterInputStream ins = (FilterInputStream) u.openStream();
		InputStreamReader isr = new InputStreamReader(ins);
		BufferedReader whiteHouse = new BufferedReader(isr);
		System.out.println(whiteHouse.readLine());
		
		
		while(aktuelleZeile != null) {
		
			aktuelleZeile = whiteHouse.readLine();
			//System.out.println(aktuelleZeile);
		if(aktuelleZeile.contains(muster)) {
			System.out.println(aktuelleZeile);
		}
		}
		sc.close();	
	}

}
